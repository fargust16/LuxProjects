    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-cookie": "^2.1.2",
    "react-dev-utils": "4.2.1",
    "react-dom": "16.2.0",
    "react-fontawesome": "1.6.1",
    "react-history": "^0.18.2",
    "react-line-clamp": "1.2.6",
    "react-router-dom": "4.2.2",
    "react-router-page-transition": "^3.0.2",
    "react-scroll": "1.7.3",
    "reset-css": "^2.2.1",
    "sw-precache-webpack-plugin": "0.11.4",
